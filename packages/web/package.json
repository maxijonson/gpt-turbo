{
    "name": "gpt-turbo-web",
    "version": "4.0.0",
    "description": "A Web app that interacts with the gpt-turbo library",
    "private": true,
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "lint": "eslint --ext .ts,.tsx src",
        "lint:strict": "npm run lint -- --max-warnings 0",
        "lint:fix": "npm run lint -- --fix",
        "tscheck": "tsc --noEmit",
        "build": "npm run lint:strict && npm run tscheck && rimraf dist && vite build",
        "dev": "vite",
        "preview": "npm run build && vite preview"
    },
    "keywords": [
        "openai",
        "chatgpt",
        "chat",
        "gpt",
        "gpt3",
        "gpt-3",
        "gpt3.5",
        "gpt-3.5",
        "gpt4",
        "gpt-4",
        "completion",
        "chatcompletion",
        "conversation",
        "conversation ai",
        "ai",
        "ml",
        "bot",
        "chatbot",
        "web",
        "web app",
        "react"
    ],
    "author": "Tristan Chin <tristan.chin@chintristan.io>",
    "homepage": "https://github.com/maxijonson/gpt-turbo/tree/develop/packages/web#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maxijonson/gpt-turbo.git"
    },
    "bugs": {
        "url": "https://github.com/maxijonson/gpt-turbo/issues"
    },
    "license": "MIT",
    "files": [
        "dist"
    ],
