        "@vitejs/plugin-react-swc": "^3.0.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.1",
        "prettier": "^2.8.4",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-pwa": "^0.16.4"
